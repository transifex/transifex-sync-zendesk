{
  "name": "Transifex Sync",
  "author": {
    "name": "Transifex",
    "email": "support@transifex.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": "nav_bar",
  "domainWhitelist": "www.transifex.com",
  "version": "1.0",
  "frameworkVersion": "1.0",
  "singleInstall": true,
  "parameters": [
    {
      "name": "tx_username",
      "type": "text",
      "required": true
    },
    {
      "name": "tx_password",
      "type": "password",
      "required": true
    },
    {
      "name": "tx_project",
      "type": "url",
      "required": true
    },
    {
      "name": "features",
      "type": "hidden",
      "default": "0"
    },{
      "name": "debug",
      "type": "hidden",
      "default": false
    },
    {
      "name": "secured",
      "type": "hidden",
      "default": true
    },
    {
      "name": "tx_timeout",
      "type": "hidden",
      "default": 10000
    }

  ]
}
